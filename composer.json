{
    "name": "EHER/PHPUnit",
    "description": "Unofficial version of PHPUnit to be handled with Composer.",
    "require": {
        "php": ">=5.3.0",
        "symfony/finder": "v2.0.16"
    },
    "bin": ["bin/dbunit", "bin/phpcov", "bin/phpcpd", "bin/phpdcd", "bin/phploc", "bin/phpunit", "bin/phpunit-skelgen"],
    "include-path": ["src"],
    "autoload": {
        "classmap": [
            "src/dbunit",
            "src/finder-facade-fix",
            "src/php",
            "src/php-code-coverage",
            "src/phpcov",
            "src/phpcpd",
            "src/phpdcd",
            "src/php-file-iterator",
            "src/phploc",
            "src/php-text-template",
            "src/php-timer",
            "src/php-token-stream",
            "src/phpunit",
            "src/phpunit-mock-objects",
            "src/phpunit-selenium",
            "src/phpunit-skeleton-generator"]
    }
}
